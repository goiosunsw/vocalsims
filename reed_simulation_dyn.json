{
    "description": "Reed simulation with vocal tract and reed dynamics",
    "version": "20200311",
    "simulation": {
        "sample rate": 48000,
        "duration": 0.1,
        "callback every": 1024
    },
    "environment": {
        "acoustic": {
            "_prefer": true,
            "speed of sound": 346.7492013525034,
            "density": 1.2,
            "viscosity": 18.84e-6
        },
        "physical": {
            "atmospheric pressure": 101500,
            "temperature": 36.5,
            "humidity": 100
        },
        "blowing pressure": {
            "_comment": "blowing rpessure is 'value' from start if ramp not enabled",
            "value": 4000,
            "ramp type": "exponential",
            "ramp enabled": false
        },
        "reed": {
            "_comment": "reed moves instantaneously if 'dynamic' is false, that means res. freq and q are not taken into account",
            "stiffness": 5e8,
            "resonance frequency": 1500,
            "quality factor": 3,
            "rest opening": 1e-5,
            "dynamic": true
        },
        "noise": {
            "enabled": false,
            "type": "white",
            "scale": 0.005
        },
        "frequency dependent losses": true,
        "vocal tract enabled": true
    },
    "perturbation": {
        "factor": 1.01,
        "time": 0.002,
        "_choices_variable": [
            "reed rest opening",
            "blowing pressure"
        ],
        "variable": "reed rest opening"
    },
    "tracts": {
        "vocal": {
            "description": "narrow tongue",
            "frequency independent losses": 0.05,
            "elements": [{
                    "type": "cylinder",
                    "length": 0.17,
                    "radius": 0.015,
                    "loss multiplier": 5
                },
                {
                    "type": "termination",
                    "kind": "open"
                }
            ]
        },
        "bore": {
            "description": "single clarinet-like cylinder",
            "frequency independent losses": 0.01,
            "elements": [{
                    "type": "cylinder",
                    "length": 0.49,
                    "radius": 0.0075,
                    "loss multiplier": 1
                },
                {
                    "type": "termination",
                    "kind": "open"
                }
            ]
        }
    } 
}